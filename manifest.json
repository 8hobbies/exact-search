{
  "manifest_version": 3,
  "name": "Exact Search",
  "version": "1.0",
  "description": "Include every word in the search result.",
  "background": {
    "service_worker": "background.js",
    "scripts": ["background.js"]
  },
  "action": {},
  "permissions": ["activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Ctrl+Shift+E"
      }
    }
  }
}
